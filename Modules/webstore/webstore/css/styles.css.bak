/*---*/
.thumbnail{
	position: relative;
    text-align: center;
    overflow: hidden;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0);
    padding-bottom: 20px;
    background: #fff;
	display: block;
    padding: 4px;
    margin-bottom: 20px;
	padding-top: 20px;
	border-radius: 0px;
    line-height: 1.42857143;
    background-color: #fff;
    border: 1px solid #ddd;
    -webkit-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}
.thumbnail:hover{
	position: relative;
    text-align: center;
    overflow: hidden;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.15);
    padding-bottom: 20px;
    background: #fff;
	display: block;
    padding: 4px;
    margin-bottom: 20px;
	padding-top: 20px;
	border-radius: 0px;
    line-height: 1.42857143;
    background-color: #fff;
    border: 1px solid #ddd;

    -webkit-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}

